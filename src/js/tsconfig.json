{
	"compilerOptions": {
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"checkJs": false,
		"incremental": true,
		"jsx": "react",
		"jsxFactory": "jsxElem",
		"lib": ["es6", "dom", "dom.iterable"],
		"moduleResolution": "node",
		"noImplicitAny": true,
		"outDir": "./dist/",
		"sourceMap": true,
		"target": "esnext"
	}
}
